{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
			"outputs": ["dist/**"]
    },
    "platform-api#build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV"
      ],
      "passThroughEnv": [],
      "outputs": []
    },
    "codegen": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    }
  }
}
